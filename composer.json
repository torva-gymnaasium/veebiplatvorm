{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/fakeobjects",
                "version": "4.17.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.17.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/iframe",
                "version": "4.17.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/iframe/releases/iframe_4.17.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "ckeditor-plugin/fakeobjects": "4.17.1",
        "ckeditor-plugin/iframe": "4.17.1",
        "ckeditor/autogrow": "^4.17",
        "ckeditor/codemirror": "^1.17",
        "ckeditor/fakeobjects": "^4.17",
        "ckeditor/image": "^4.17",
        "ckeditor/link": "^4.17",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^3.5",
        "drupal/bartik": "^1.0",
        "drupal/block_list_override": "^1.0",
        "drupal/calendar_link": "^3.0",
        "drupal/ckeditor_iframe": "^3.0@beta",
        "drupal/color": "^1.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.2",
        "drupal/config_split": "^2.0",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/crop": "^2.1",
        "drupal/default_content": "2.0.x-dev@dev",
        "drupal/devel": "^5.0",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/disable_language": "^1.0@RC",
        "drupal/dwr": "^1.5",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/exif_orientation": "^1.3",
        "drupal/externalauth": "^2.0",
        "drupal/fakeobjects": "^2.0",
        "drupal/field_group": "^3.1",
        "drupal/file_delete": "^2.0",
        "drupal/focal_point": "^2.0",
        "drupal/genpass": "^2.0",
        "drupal/git_status": "^1.0",
        "drupal/google_tag": "^1.8",
        "drupal/hal": "^1.0 | ^2.0",
        "drupal/honeypot": "^2.2",
        "drupal/language_switcher_extended": "^1.0@beta",
        "drupal/layout_builder_at": "^2.15",
        "drupal/layout_builder_restrictions": "^2.17",
        "drupal/linkit": "^6.0",
        "drupal/login_redirect_per_role": "^1.7",
        "drupal/maillog": "^1.0@beta",
        "drupal/matomo": "^1.25",
        "drupal/maxlength": "^2.1",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/media_library_edit": "^3.0",
        "drupal/menu_admin_per_menu": "1.x-dev@dev",
        "drupal/menu_block": "^1.14",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/metatag": "^2.1",
        "drupal/noreqnewpass": "^1.3",
        "drupal/openid_connect": "^3.0@alpha",
        "drupal/openid_connect_harid": "^3.0",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.8",
        "drupal/quickedit": "^1.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/rdf": "^2.1",
        "drupal/readonly_field_widget": "^1.4",
        "drupal/recaptcha_v3": "^1.7",
        "drupal/redirect": "^1.11",
        "drupal/robotstxt": "^1.4",
        "drupal/role_delegation": "1.x-dev@dev",
        "drupal/scheduler": "^2.2",
        "drupal/search_api": "^1.38",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/select2": "^1.13",
        "drupal/seven": "^1.0",
        "drupal/simple_recaptcha": "^1.0@beta",
        "drupal/simple_sitemap": "^4.2",
        "drupal/sitemap": "^2.0@beta",
        "drupal/svg_image": "^3.0",
        "drupal/taxonomy_menu_ui": "^3.0",
        "drupal/time_field": "^2.0",
        "drupal/token": "^1.7",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/twig_tweak": "^3.3",
        "drupal/userprotect": "1.x-dev@dev",
        "drupal/views_ajax_history": "^1.7",
        "drupal/webform": "^6.2",
        "drupal/weight": "^3.3",
        "drush/drush": "^12",
        "mglaman/composer-drupal-lenient": "^1.0",
        "popperjs/popperjs": "^2.11",
        "tippyjs/5.x": "^5.2",
        "tippyjs/6.x": "^6.2",
        "tippyjs/tippyjs": "^6.3",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/core-project-message": true,
            "mglaman/composer-drupal-lenient": true
        },
        "process-timeout": 300
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"if (file_exists('./web/libraries/algolia.places')) { \\$files = new RecursiveIteratorIterator(new RecursiveDirectoryIterator('./web/libraries/algolia.places')); foreach (\\$files as \\$file) { if (\\$file->isFile()) { file_put_contents(\\$file, str_replace('cdn.polyfill.io', 'cdnjs.cloudflare.com/polyfill', file_get_contents(\\$file))); } } }\"",
            "rm -f web/libraries/choices/public/index.html"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/robots.txt": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/development.settings.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/dropzonejs/composer.libraries.json",
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/media_bulk_upload": {
                "Provide a way to add alt text when doing bulk upload [media_bulk_upload] https://www.drupal.org/project/media_bulk_upload/issues/3023825": "https://www.drupal.org/files/issues/2023-12-20/media_bulk_upload-3023825-64.patch"
            },
            "drupal/dropzonejs": {
                "Provide a way to add alt text when doing bulk upload [dropzonejs] https://www.drupal.org/project/media_bulk_upload/issues/3023825": "https://www.drupal.org/files/issues/2020-10-05/dropzonejs-3023825-27.patch"
            },
            "drupal/dwr": {
                "Automated Drupal 10 compatibility fixes https://www.drupal.org/project/dwr/issues/3287107": "https://www.drupal.org/files/issues/2022-06-15/dwr.1.x-dev.rector.patch"
            },
            "drupal/genpass" : {
                "The installed version of the /Generate Password/ module is too old to update https://www.drupal.org/project/genpass/issues/3482498" : "https://www.drupal.org/files/issues/2024-10-22/genpass_remove_error_3482498-4.patch"
            }
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/dwr"]
        }
    }
}
