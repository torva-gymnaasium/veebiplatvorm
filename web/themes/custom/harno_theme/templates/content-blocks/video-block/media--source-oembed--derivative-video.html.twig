{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 */
#}
<div class="video-block" aria-label="{{ " Video"|t({},{'context':'Vimeo or youtube'}) }}" >
  {% if content %}
    {{ content }}
  {% endif %}
</div>
